#include <iostream> //12577
#include <string>
#include <bits/stdc++.h>
using namespace std;

int main() {
	string str;
	int i=1;
	while (getline(cin, str)){
		
		if (str == "Hajj") {
			cout << "Case " << i << ": " << "Hajj-e-Akbar\n";
		}
		if (str == "Umrah"){
			cout << "Case " << i << ": " << "Hajj-e-Asghar\n";
		}
		i++;
	}
}
